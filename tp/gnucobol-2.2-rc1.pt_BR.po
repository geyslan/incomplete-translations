#: cobc/cobc.c:2188
#, c-format
msgid "Usage: %s [options]... file..."
msgstr "Uso: %s [opções]... arquivo..."

#: cobc/cobc.c:2191 bin/cobcrun.c:120
msgid "Options:"
msgstr "Opções:"

#: cobc/cobc.c:2192
msgid "  -h, -help             display this help and exit"
msgstr "  -h, -help             exibe esta ajuda e sai"

#: cobc/cobc.c:2193
msgid "  -V, -version          display compiler version and exit"
msgstr "  -V, -version          exibe a versão do compilador e sai"

#: cobc/cobc.c:2194
msgid ""
"  -i, -info             display compiler information (build/environment)\n"
"                        and exit"
msgstr ""
"  -i, -info             exibe informações do compilador (compilação/ambiente)"
"\n"
"                        e sai"

#: cobc/cobc.c:2196
msgid ""
"  -v, -verbose          display compiler version and the commands\n"
"                        invoked by the compiler"
msgstr ""
"  -v, -verbose          exibe versão do compilador e os comandos\n"
"                        chamado pelo compilador"

#: cobc/cobc.c:2198
msgid ""
"  -vv, -verbose=2       like -v but additional pass verbose option\n"
"                        to assembler/compiler"
msgstr ""
"  -vv, -verbose=2       como -v, mas opção verbosa adicional passada\n"
"                        para o montador/compilador"

#: cobc/cobc.c:2200
msgid ""
"  -vvv, -verbose=3      like -vv but additional pass verbose option\n"
"                        to linker"
msgstr ""
"  -vvv, -verbose=3      como -v, mas opção verbosa adicional passada\n"
"                        para o vinculador"

#: cobc/cobc.c:2202
msgid "  -q, -brief            reduced displays, commands invoked not shown"
msgstr ""
"  -q, -brief            exibições reduzidas, comandos chamados não mostrados"

#: cobc/cobc.c:2203
msgid "  -###                 like -v but commands not executed"
msgstr "  -###                  como -v, mas comandos não são executados"

#: cobc/cobc.c:2204
msgid "  -x                    build an executable program"
msgstr "  -x                    compila um programa executável"

#: cobc/cobc.c:2205
msgid "  -m                    build a dynamically loadable module (default)"
msgstr ""
"  -m                    compila um módulo carregável dinamicamente (padrão)"

#: cobc/cobc.c:2206
msgid "  -j [<args>], -job[=<args>]\trun program after build, passing <args>"
msgstr ""
"  -j [<args>], -job[=<args>]\n"
"                        executa o programa após compilar, passando <args>"

#: cobc/cobc.c:2207
msgid ""
"  -std=<dialect>        warnings/features for a specific dialect\n"
"                        <dialect> can be one of:\n"
"                        default, cobol2014, cobol2002, cobol85, xopen,\n"
"                        ibm-strict, ibm, mvs-strict, mvs,\n"
"                        mf-strict, mf, bs2000-strict, bs2000,\n"
"                        acu-strict, acu, rm-strict, rm;\n"
"                        see configuration files in directory config"
msgstr ""
"  -std=<dialeto>        avisos/recursos para um dialeto específico\n"
"                        <dialeto> pode ser um entre:\n"
"                        default, cobol2014, cobol2002, cobol85, xopen,\n"
"                        ibm-strict, ibm, mvs-strict, mvs,\n"
"                        mf-strict, mf, bs2000-strict, bs2000,\n"
"                        acu-strict, acu, rm-strict, rm;\n"
"                        veja arquivos de configuração no diretório de config"

#: cobc/cobc.c:2214
msgid "  -F, -free             use free source format"
msgstr "  -F, -free             usa formato de fonte livre"

#: cobc/cobc.c:2215
msgid "  -fixed                use fixed source format (default)"
msgstr "  -fixed                usa formato de fonte fixa (padrão)"

#: cobc/cobc.c:2216
msgid "  -O, -O2, -O3, -Os     enable optimization"
msgstr "  -O, -O2, -O3, -Os     habilita otimização"

#: cobc/cobc.c:2217
msgid "  -g                    enable C compiler debug / stack check / trace"
msgstr ""
"  -g                    habilita depuração, verificação de pilha e\n"
"                        rastreamento de compilação C"

#: cobc/cobc.c:2218
msgid "  -d, -debug            enable all run-time error checking"
msgstr ""
"  -d, -debug            habilita toda verificação de erro de tempo de "
"execução"

#: cobc/cobc.c:2219
msgid "  -o <file>             place the output into <file>"
msgstr "  -o <arquivo>          coloca a saída no <arquivo>"

#: cobc/cobc.c:2220
msgid ""
"  -b                    combine all input files into a single\n"
"                        dynamically loadable module"
msgstr ""
"  -b                    combina todos os arquivos de entrada\n"
"                        em um único módulo carregável dinamicamente"

#: cobc/cobc.c:2222
msgid "  -E                    preprocess only; do not compile or link"
msgstr "  -E                    pré-processa apenas; não compila nem vincula"

#: cobc/cobc.c:2223
msgid "  -C                    translation only; convert COBOL to C"
msgstr "  -C                    tradução, apenas; converte COBOL para C"

#: cobc/cobc.c:2224
msgid "  -S                    compile only; output assembly file"
msgstr "  -S                    compila, apenas; gera arquivo de montagem"

#: cobc/cobc.c:2225
msgid "  -c                    compile and assemble, but do not link"
msgstr "  -c                    compila e monta, mas não vincula"

#: cobc/cobc.c:2226
msgid ""
"  -T <file>             generate and place a wide program listing into <file>"
msgstr ""
"  -T <arquivo>          gera e coloca uma listagem ampla de programas em "
"<arquivo>"

#: cobc/cobc.c:2227
msgid ""
"  -t <file>             generate and place a program listing into <file>"
msgstr ""
"  -t <arquivo>          gera e coloca uma listagem ampla de programas em "
"<arquivo>"

#: cobc/cobc.c:2228
msgid "  --tlines=<lines>      specify lines per page in listing, default = 55"
msgstr ""
"  --tlines=<linhas>     especifica linhas por página na lista, padrão = 55"

#: cobc/cobc.c:2229
msgid "  --tsymbols            specify symbols in listing"
msgstr "  --tsymbols            especifica símbolos na listagem"

#: cobc/cobc.c:2230
msgid "  -P[=<dir or file>]    generate preprocessed program listing (.lst)"
msgstr ""
"  -P[=<dir ou arquivo>] gera listagem de programas pré-processados (.lst)"

#: cobc/cobc.c:2232
msgid ""
"  -Xref                 generate cross reference through 'cobxref'\n"
"                        (V. Coen's 'cobxref' must be in path)"
msgstr ""
"  -Xref                 gera referência cruzada por meio de \"cobxref\"\n"
"                        (\"cobxref\" do V. Coen deve estar no caminho)"

#: cobc/cobc.c:2235
msgid "  -Xref                 specify cross reference in listing"
msgstr "  -Xref                 especifica referência cruzada na listagem"

#: cobc/cobc.c:2237
msgid "  -I <directory>        add <directory> to copy/include search path"
msgstr ""
"  -I <diretório>        adiciona <diretório> para o caminho de pesquisa\n"
"                        de copy/include"

#: cobc/cobc.c:2238
msgid "  -L <directory>        add <directory> to library search path"
msgstr ""
"  -L <diretório>        adiciona <diretório> ao caminho de pesquisa\n"
"                        de bibliotecas"

#: cobc/cobc.c:2239
msgid "  -l <lib>              link the library <lib>"
msgstr "  -l <lib>              vincula a biblioteca <lib>"

#: cobc/cobc.c:2240
msgid "  -A <options>          add <options> to the C compile phase"
msgstr "  -A <opções>           adiciona <opções> à fase de compilação C"

#: cobc/cobc.c:2241
msgid "  -Q <options>          add <options> to the C link phase"
msgstr "  -Q <opções>           adiciona <opções> à fase de vinculação C"

#: cobc/cobc.c:2242
msgid "  -D <define>           define <define> for COBOL compilation"
msgstr "  -D <define>           define <define> para compilação COBOL"

#: cobc/cobc.c:2243
msgid "  -K <entry>            generate CALL to <entry> as static"
msgstr "  -K <entrada>          gera chamada para <entrada> como estática"

#: cobc/cobc.c:2244
msgid "  -conf=<file>          user-defined dialect configuration; see -std"
msgstr ""
"  -conf=<arquivo>       configuração de dialeto definida pelo usuário; ver "
"-std"

#: cobc/cobc.c:2245
msgid "  -list-reserved        display reserved words"
msgstr "  -list-reserved        exibe palavras reservadas"

#: cobc/cobc.c:2246
msgid "  -list-intrinsics      display intrinsic functions"
msgstr "  -list-intrinsics      exibe funções intrínsecas"

#: cobc/cobc.c:2247
msgid "  -list-mnemonics       display mnemonic names"
msgstr "  -list-mnemonics       exibe nomes mnemônicos"

#: cobc/cobc.c:2248
msgid "  -list-system          display system routines"
msgstr "  -list-system          exibe rotinas de sistema"

#: cobc/cobc.c:2249
msgid ""
"  -save-temps[=<dir>]   save intermediate files\n"
"                        - default: current directory"
msgstr ""
"  -save-temps[=<dir>]   salva arquivos intermediários\n"
"                        - padrão: diretório atual"

#: cobc/cobc.c:2251
msgid "  -ext <extension>      add file extension for resolving COPY"
msgstr "  -ext <extensão>       adiciona extensão de arquivo para resolver COPY"

#: cobc/cobc.c:2255
msgid "  -W                    enable all warnings"
msgstr "  -W                    habilita todos avisos"

#: cobc/cobc.c:2256
msgid ""
"  -Wall                 enable most warnings (all except as noted below)"
msgstr ""
"  -Wall                 habilita a maioria dos aviso (todos, exceto como "
"observado abaixo)"

#: cobc/cobc.c:2257
msgid "  -Wno-<warning>        disable warning enabled by -W or -Wall"
msgstr "  -Wno-<aviso>          desabilita aviso habilitado por -W ou -Wall"

#: cobc/cobc.c:2268
msgid "  -Werror               treat all warnings as errors"
msgstr "  -Werror               trata todos avisos como erros"

#: cobc/cobc.c:2269
msgid "  -Werror=<warning>     treat specified <warning> as error"
msgstr "  -Werror=<aviso>       trata <aviso> especificado como erro"

#: cobc/cobc.c:2290 cobc/cobc.c:2294
msgid "<value>"
msgstr "<valor>"

#: cobc/cobc.c:2298
msgid "<support>"
msgstr "<suporte>"

#: cobc/cobc.c:2306
msgid "where <support> is one of the following:"
msgstr "onde <suporte> é um dentre os seguintes:"

#: cobc/cobc.c:2309
msgid "word to be taken out of the reserved words list"
msgstr "palavra a ser retirada da lista de palavras reservadas"

#: cobc/cobc.c:2309 cobc/cobc.c:2310
msgid "<word>"
msgstr "<palavra>"

#: cobc/cobc.c:2310
msgid "word to be added to reserved words list"
msgstr "palavra a ser adicionada à lista de palavras reservadas"

#: cobc/cobc.c:2311
msgid "word to be added to reserved words list as alias"
msgstr "palavra a ser adicionada à lista de palavras reservadas como apelido"

#: cobc/cobc.c:2311
msgid "<word>:<alias>"
msgstr "<palavra>:<apelido>"

#: cobc/cobc.c:2316 bin/cobcrun.c:136
#, c-format
msgid ""
"Report bugs to: %s\n"
"or (preferably) use the issue tracker via the home page."
msgstr ""
"Relate erros para: %s\n"
"ou (preferivelmente) usar o rastreador de erros pela página."

#: cobc/cobc.c:2319 bin/cobcrun.c:139
msgid "GnuCOBOL home page: <http://www.gnu.org/software/gnucobol/>"
msgstr "Página do GnuCOBOL: <http://www.gnu.org/software/gnucobol/>"

#: cobc/cobc.c:2320 bin/cobcrun.c:140
msgid "General help using GNU software: <http://www.gnu.org/gethelp/>"
msgstr "Ajuda para uso de softwares GNU: <http://www.gnu.org/gethelp/>"

#: cobc/cobc.c:2326
msgid "only one of options 'E', 'S', 'C', 'c' may be specified"
msgstr ""
"apenas uma das opções \"E\", \"S\", \"C\" e \"c\" podem ser especificadas"

#: cobc/cobc.c:2332
msgid "only one of options 'm', 'x', 'b' may be specified"
msgstr "apenas uma das opções \"m\", \"x\" e \"b\" podem ser especificadas"

#: cobc/cobc.c:2350
#, c-format
msgid "'%s' is not an intrinsic function"
msgstr "\"%s\" não é uma função intrínseca"

#: cobc/cobc.c:2523
#, c-format
msgid "invalid parameter -std=%s"
msgstr "Parâmetro inválido -std=%s"

#: cobc/cobc.c:2544
msgid "loading standard configuration file 'default.conf'"
msgstr "carregando arquivo de configuração padrão \"default.conf\""

#: cobc/cobc.c:2678
msgid "invalid output file name"
msgstr "nome inválido de arquivo de saída"

#: cobc/cobc.c:2760
